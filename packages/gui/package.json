{
  "name": "labeler",
  "version": "0.0.1",
  "description": "A webapp for labelling data in your browser, no setup required.",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "npm start",
    "start": "npx rspack serve",
    "build": "npx rspack build",
    "build:domain": "npm run clean && npm run build"
  },
  "keywords": [
    "mithril",
    "materialize-css",
    "typescript"
  ],
  "license": "MIT",
  "app": "src/app.ts",
  "target": "web",
  "dependencies": {
    "idb": "^8.0.2",
    "material-icons": "^1.13.13",
    "materialize-css": "^1.0.0",
    "meiosis-setup": "^6.2.3",
    "mithril": "^2.2.13",
    "mithril-materialized": "^1.3.5",
    "mithril-ui-form": "^1.10.15",
    "mithril-ui-form-rating-plugin": "^0.7.6",
    "papaparse": "^5.5.2",
    "translate.js": "^1.3.2"
  },
  "devDependencies": {
    "@rspack/cli": "^1.2.2",
    "@rspack/core": "^1.2.2",
    "@types/materialize-css": "^1.0.14",
    "@types/mithril": "^2.2.7",
    "@types/papaparse": "^5.3.15",
    "dotenv": "16.4.7",
    "rimraf": "^6.0.1",
    "sass": "^1.83.4",
    "sass-loader": "16.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
